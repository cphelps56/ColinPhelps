@using System.Web.Optimization;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Colin Phelps Personal Site</title>
    @Styles.Render("~/bundles/sitecss")
    @Styles.Render("~/bundles/bootstrap/css")
    @Styles.Render("~/bundles/fontawesome")
</head>
<body>
    @Html.Action("Header", "Nav")
    <div class="sitebody">
        @RenderBody()
    </div>
    @Html.Action("Footer", "Nav")

    @Scripts.Render("~/bundles/bootstrapscripts")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/javascript")
    
    @RenderSection("scripts", required: false)
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-100321309-1', 'auto');
        ga('send', 'pageview');
    </script>
    <script>
        (function (helper) {
            $(function () {
                helper.onready();
            });
        })(analyticsHelper);
    </script>
</body>
</html>